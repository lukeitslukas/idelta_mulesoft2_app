{
    "pages": {
        "configuration": {
            "tabs": [
                {
                    "name": "account",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Account Name",
                                "field": "name"
                            },
                            {
                                "label": "Username",
                                "field": "clientid"
                            },
                            {
                                "label": "Password",
                                "field": "clientsecret"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Account Name",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique identifier for the account",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Username or Client ID",
                            "field": "clientid",
                            "help": "Username or Client ID (if using MFA) for the mulesoft account",
                            "required": true,
                            "encrypted": true
                        },
                        {
                            "type": "text",
                            "label": "Password or Client Secret",
                            "field": "clientsecret",
                            "help": "Password or Client Secret (if using MFA) for the mulesoft account",
                            "required": true,
                            "encrypted": true
                        }
                    ],
                    "title": "Accounts"
                },
                {
                    "name": "organisation",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Organisation Name",
                                "field": "name"
                            },
                            {
                                "label": "Organisation ID",
                                "field": "organisationid"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Organisation Name",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique identifier for the organisation",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Organisation ID",
                            "field": "organisationid",
                            "help": "The Organisation ID (found from the Discovery input)",
                            "required": true,
                            "encrypted": true
                        }
                    ],
                    "title": "Organisations"
                },
                {
                    "name": "environment",
                    "table": {
                        "actions": [
                            "edit",
                            "delete",
                            "clone"
                        ],
                        "header": [
                            {
                                "label": "Environment Name",
                                "field": "name"
                            },
                            {
                                "label": "Environment ID",
                                "field": "environmentid"
                            }
                        ]
                    },
                    "entity": [
                        {
                            "type": "text",
                            "label": "Environment Name",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique identifier for the environment",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Environment ID",
                            "field": "environmentid",
                            "help": "The Environment ID (found from the Discovery input)",
                            "required": true,
                            "encrypted": true
                        }
                    ],
                    "title": "Environments"
                },
                {
                    "type": "loggingTab"
                },
                {
                    "name": "proxy",
                    "title": "Proxy",
                    "entity": [
                        {
                            "type": "checkbox",
                            "label": "Enable",
                            "field": "proxy_enabled"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Proxy Type",
                            "options": {
                                "disableSearch": true,
                                "autoCompleteFields": [
                                    {
                                        "value": "http",
                                        "label": "http"
                                    },
                                    {
                                        "value": "socks4",
                                        "label": "socks4"
                                    },
                                    {
                                        "value": "socks5",
                                        "label": "socks5"
                                    }
                                ]
                            },
                            "defaultValue": "http",
                            "field": "proxy_type"
                        },
                        {
                            "type": "text",
                            "label": "Host",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Max host length is 4096",
                                    "minLength": 0,
                                    "maxLength": 4096
                                }
                            ],
                            "field": "proxy_url"
                        },
                        {
                            "type": "text",
                            "label": "Port",
                            "validators": [
                                {
                                    "type": "number",
                                    "range": [
                                        1,
                                        65535
                                    ]
                                }
                            ],
                            "field": "proxy_port"
                        },
                        {
                            "type": "text",
                            "label": "Username",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Max length of username is 50",
                                    "minLength": 0,
                                    "maxLength": 50
                                }
                            ],
                            "field": "proxy_username"
                        },
                        {
                            "type": "text",
                            "label": "Password",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Max length of password is 8192",
                                    "minLength": 0,
                                    "maxLength": 8192
                                }
                            ],
                            "encrypted": true,
                            "field": "proxy_password"
                        },
                        {
                            "type": "checkbox",
                            "label": "Reverse DNS resolution",
                            "field": "proxy_rdns"
                        }
                    ],
                    "options": {
                        "saveValidator": "function(formData) { if(!formData.proxy_enabled || formData.proxy_enabled === '0') {return true; } if(!formData.proxy_url) { return 'Proxy Host can not be empty'; } if(!formData.proxy_port) { return 'Proxy Port can not be empty'; } return true; }"
                    }
                }
            ],
            "title": "Configuration",
            "description": "Set up your add-on"
        },
        "inputs": {
            "services": [
                {
                    "name": "discovery_input",
                    "entity": [
                        {
                            "type": "text",
                            "label": "Input Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the discovery input.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Index",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "index",
                            "help": "The index to ingest discovery data to",
                            "defaultValue": "mulesoft",
                            "required": true
                        },
                        {
                            "type": "interval",
                            "field": "interval",
                            "label": "Interval",
                            "help": "How often to check for new enviornments/organisations, in seconds.",
                            "required": true,
                            "defaultValue": "86400"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Account",
                            "options": {
                                "referenceName": "account"
                            },
                            "help": "Account to use for this input.",
                            "field": "account",
                            "required": true
                        }
                    ],
                    "title": "Discovery Input"
                },
                {
                    "name": "app_logs_input",
                    "entity": [
                        {
                            "type": "text",
                            "label": "Input Name",
                            "validators": [
                                {
                                    "type": "regex",
                                    "errorMsg": "Input Name must begin with a letter and consist exclusively of alphanumeric characters and underscores.",
                                    "pattern": "^[a-zA-Z]\\w*$"
                                },
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "name",
                            "help": "A unique name for the app logs input.",
                            "required": true
                        },
                        {
                            "type": "text",
                            "label": "Index",
                            "validators": [
                                {
                                    "type": "string",
                                    "errorMsg": "Length of input name should be between 1 and 100",
                                    "minLength": 1,
                                    "maxLength": 100
                                }
                            ],
                            "field": "index",
                            "help": "The index to ingest log data to",
                            "defaultValue": "mulesoft",
                            "required": true
                        },
                        {
                            "type": "interval",
                            "field": "interval",
                            "label": "Interval",
                            "help": "How often to grab application logs, in seconds.",
                            "required": true,
                            "defaultValue": "86400"
                        },
                        {
                            "type": "singleSelect",
                            "label": "Account",
                            "options": {
                                "referenceName": "account"
                            },
                            "help": "Account to use for this input.",
                            "field": "account",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "label": "Organisation ID",
                            "options": {
                                "referenceName": "organisation"
                            },
                            "help": "Organisation to use for this input.",
                            "field": "organisation",
                            "required": true
                        },
                        {
                            "type": "singleSelect",
                            "label": "Environment ID",
                            "options": {
                                "referenceName": "environment"
                            },
                            "help": "Environment to use for this input.",
                            "field": "environment",
                            "required": true
                        }
                    ],
                    "title": "App Logs Input"
                }
            ],
            "title": "Inputs",
            "description": "Manage your data inputs",
            "table": {
                "actions": [
                    "edit",
                    "enable",
                    "delete",
                    "clone"
                ],
                "header": [
